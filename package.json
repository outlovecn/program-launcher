{
  "name": "program-launcher",
  "version": "0.0.0",
  "main": "./dist/program-launcher.cjs",
  "module": "./dist/program-launcher.mjs",
  "exports": {
    ".": {
      "require": "./dist/program-launcher.cjs",
      "import": "./dist/program-launcher.mjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf build/**/* && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts && copyfiles ./package.json build",
    "lint:scripts": "eslint . --ext .ts",
    "format:scripts": "prettier . --write",
    "format": "npm run format:scripts",
    "prepare": "husky install && husky set .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky",
    "release": "pnpm build && changelogen --release --push && npm publish"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "changelogen": "^0.5.5",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "happy-dom": "^14.12.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  }
}
